<h1>Edit post</h1>

<% form_for(post, {action: path_to.post(post), method: 'PUT', id: "post_form"}, function (form) { %>
	<%- partial('posts/form.ejs', {locals: {form: form, post: post}}) %>
	<div class="page-actions">
		<%- form.submit('Update post', {class: 'btn primary'}) %>
		<%- link_to('Cancel', path_to.posts, {class: 'btn'}) %>
	</div>
<% });%>
